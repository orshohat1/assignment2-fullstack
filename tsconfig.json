{
    "compilerOptions": {
      "target": "ES6", 
      "module": "commonjs", 
      "strict": true,
      "esModuleInterop": true, 
      "resolveJsonModule": true, 
      "outDir": "./dist", 
      "rootDir": ".", 
      "skipLibCheck": true, 
      "types": ["jest", "node"], 
    },
    "include": [
      "**/*.ts", 
      "tests/**/*.ts" 
    ],
    "exclude": [
      "node_modules", 
      "dist", 
      "**/*.test.ts" 
    ]
  }
  